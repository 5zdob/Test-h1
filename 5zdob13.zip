import zipfile

# Create a list of ZIP codes
zip_codes = [f"{i:05d}" for i in range(10000, 100000)]

# Create a ZIP file on disk
with zipfile.ZipFile('zip_codes.zip', 'w') as z:
    # Write each ZIP code as a separate file
    for zip_code in zip_codes:
        z.writestr(f"{zip_code}.txt", zip_code)
